<Defs>
    <HediffDef ParentName="ChronicDiseaseBase">
        <defName>MSS_Gen_CompromisedImmunity</defName>
        <label>Compromised Immunity</label>
        <description>This persons immune system is compromised due to old age.</description>
        <initialSeverity>0.1</initialSeverity>
        <maxSeverity>1.0</maxSeverity>
        <tendable>false</tendable>
        <chronic>true</chronic>
        <stages>
            <li>
                <label>initial</label>
            </li>
            <li>
                <label>minor</label>
                <minSeverity>0.1</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.9</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>minor</label>
                <minSeverity>0.2</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.8</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>minor</label>
                <minSeverity>0.3</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.7</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>major</label>
                <minSeverity>0.4</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.6</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>major</label>
                <minSeverity>0.5</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.5</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>major</label>
                <minSeverity>0.6</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.4</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>extreme</label>
                <minSeverity>0.7</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.3</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>extreme</label>
                <minSeverity>0.8</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.2</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>extreme</label>
                <minSeverity>0.9</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.1</ImmunityGainSpeed>
                </statFactors>
            </li>
            <li>
                <label>complete failure</label>
                <minSeverity>1.0</minSeverity>
                <statFactors>
                    <ImmunityGainSpeed>0.0</ImmunityGainSpeed>
                </statFactors>
            </li>
        </stages>
